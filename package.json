{
    "name": "sequence",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev:local:client": "webpack-dev-server --config ./webpack.dev.config.js",
        "dev:local:server": "nodemon --config .nodemonrc ./src/server/index.js",
        "dev:local": "run-p dev:local:server dev:local:client",
        "db:migrate": "knex migrate:latest",
        "db:rollback": "knex migrate:rollback",
        "db:seed": "knex seed:run",
        "test": "jest --watch --NODE_ENV=test",
        "clean": "rm -rf ./build && mkdir ./build",
        "build:client": "webpack --config ./webpack.prod.config.js",
        "build": "babel ./src -s --copy-files --out-dir ./build",
        "start": "npm run build && node ./build/server/index.js",
        "heroku-postbuild": "npm install && npm install --dev && npm run build:client"
    },
